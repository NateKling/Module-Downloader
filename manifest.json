{
    "manifest_version": 3,
    "name": "Save HTML Text and Headers",
    "version": "1.0",
    "description": "A Chrome extension to save all HTML text and headers.",
    "permissions": ["activeTab", "storage","scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        
      },
      "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+B",
            "mac": "Command+B"
          }
        }
      }
    
    
  }

  